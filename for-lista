Ex. 15:


            Console.WriteLine("Entre com uma altura: ");
            int alt = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Entre com uma largura: ");
            int larg = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("\n");
            for (int i = 1; i<=alt;i++)
            {
                for (int j = 1; j <= larg; j++)
                {
                    Console.Write("* ");
                }
                Console.WriteLine("\n");
            }
            Console.ReadLine();

            
